{
  "name": "@cdk-as/cli",
  "description": "A CLI for the AssemblyScript CLI",
  "version": "1.0.0",
  "author": "Rick Porter @rckprtr",
  "bin": {
    "cdk-as": "./bin/run"
  },
  "bugs": "https://github.com/rckprtr/cdk-as/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
